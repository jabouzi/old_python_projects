a = 2.0
b = 1
while b < 60:
	a = a*a
	b = b + 1
	print a, type(a), "b = ", b
print "FIN !"
print a,b
c = "skanderjabouzi"
d = 0
while d < len(c):
	if c[d] == "e":
		print "la lettre no : ",d,"est un e"
	d = d + 1
	